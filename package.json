  "scripts": {
    "client:start": "webpack-dev-server --open --mode development",
    "client:clean": "rm -rf dist",
    "client:build": "webpack --mode production",
    "app:build": "yarn client:clean && yarn client:build",
    "dev:deploy": "yarn app:build && node src/server.js",
    "prod:deploy": "yarn app:build && pm2 start --name 'prod:appname' src/server.js",
    "prod:restart": "yarn app:build && pm2 restart 'prod:appname'"
  },
